
<MBreadcrumbs Items="@GetItems()"></MBreadcrumbs>

@code {
    [Parameter]
    public List<KeyValuePair<string, int>> TagList { get; set; }

    public List<BreadcrumbItem> GetItems()
    {
        return TagList.Select(s => new BreadcrumbItem
            {
                Text = s.Key,
                Href = "/tags/index/" + s.Value
            }).ToList();
    }

}
