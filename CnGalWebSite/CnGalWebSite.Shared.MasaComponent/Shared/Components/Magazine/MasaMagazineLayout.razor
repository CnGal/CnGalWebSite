<div class="@($"magazine-layout magazine-style {Class}")" id="@LayoutId">
    @ChildContent
</div>
@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }
    [Parameter]
    [EditorRequired]
    public string LayoutId { get; set; }
    [Parameter]
    public string Class { get; set; }

}
