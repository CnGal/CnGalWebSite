
<section class="content" id="@Title">
    <h2>
        <MIcon Large Color="primary" Class="me-4">@Icon.ToIconString()</MIcon><span class="primary-text"> @Title</span>
    </h2>
    <div class="container">
        @ChildContent
    </div>
</section>


@code {
    [Parameter]
    public string Title { get; set; }
    [Parameter]
    public IconType Icon { get; set; }
    [Parameter]
    public RenderFragment ChildContent { get; set; }

}
