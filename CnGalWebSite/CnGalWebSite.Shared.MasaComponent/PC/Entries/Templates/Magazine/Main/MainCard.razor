
<CnGalWebSite.Shared.MasaComponent.Shared.Components.Magazine.MasaMagazineContext Id="@Id" Class="main-card">
    @if (string.IsNullOrWhiteSpace(Model.Pictures.FirstOrDefault()?.Pictures?.FirstOrDefault()?.Url) == false)
    {
        <img src="@Model.Pictures.FirstOrDefault()?.Pictures?.FirstOrDefault()?.Url" loading="lazy" />
    }
</CnGalWebSite.Shared.MasaComponent.Shared.Components.Magazine.MasaMagazineContext>

@code {
    [Parameter]
    public EntryIndexViewModel Model { get; set; }
    [Parameter]
    public string Id { get; set; }

}
