@inject NavigationManager NavigationManager

<CnGalWebSite.Shared.MasaComponent.PC.Home.Cards.Shared.IconChip Outlined Color="info" Icon="@IconType.Lottery" Text="@Name" OnClick="OnClick" />


 @code {
    [Parameter]
    public string Name { get; set; }
    [Parameter]
    public long Id { get; set; }

    public void OnClick()
    {

        NavigationManager.NavigateTo($"/lotteries/index/{Id}");

    }
}
