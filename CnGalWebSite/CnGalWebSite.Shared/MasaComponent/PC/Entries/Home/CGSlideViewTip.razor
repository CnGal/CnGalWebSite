@inject NavigationManager NavigationManager
@inject IDataCacheService _dataCacheService

<CnGalWebSite.Shared.MasaComponent.Shared.Components.MasaTitleContainer Title="@(Model.Name+" CG预览")" Icon="@IconType.PhotoAlbum.ToIconString()" ShowMoreButton OnClickMore="OnClickMore">
    <CnGalWebSite.Shared.MasaComponent.Shared.Components.ImageSlideGroup Model="Model.Pictures" />
</CnGalWebSite.Shared.MasaComponent.Shared.Components.MasaTitleContainer>

@code {
    [Parameter]
    public GameCGModel Model { get; set; }


    public void OnClickMore()
    {
        NavigationManager.NavigateTo("/entries/index/" + Model.Id);
        
    }
}
